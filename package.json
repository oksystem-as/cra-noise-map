{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "start-prod": "http-server .\" ",
    "lite": "lite-server -c bs-config.js",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "lite-server-test": "lite-server --config=liteserver-test-config.json",
    "test": "tsc && concurrently \"npm run tsc:w\" \"npm run lite-server-test\" "
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",
    "angular2-in-memory-web-api": "0.0.15",
    "angular2-logger": "0.4.5",
    "angular2-web-worker": "0.0.3",
    "bootstrap": "3.3.6",
    "bootstrap-slider": "9.2.0",
    "chart.js": "2.2.2",
    "core-js": "2.4.1",
    "http-server": "0.9.0",
    "ng2-bootstrap": "1.1.14",
    "ng2-charts": "1.3.0",
    "ng2-dropdown": "0.0.13",
    "ng2-responsive": "0.4.6",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "zone.js": "0.6.23"
